{
  "venvPath": ".",
  "venv": "venv",
  "reportMissingImports": false,
  "reportMissingModuleSource": false,
  "reportGeneralTypeIssues": false,
  "pythonVersion": "3.12",
  "typeCheckingMode": "off",
  "useLibraryCodeForTypes": true,
  "stubPath": "./typings",
  "ignore": [
    "**/node_modules",
    "**/__pycache__",
    "**/.git"
  ],
  "extraPaths": [
    "./venv/lib/python3.12/site-packages",
    "."
  ]
}